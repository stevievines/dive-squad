<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
        <th>Dive</th>
        <th>Height</th>
        <th>Description</th>
      </tr>
    </thead>
    <tbody>
      <% list.dives.each do |dive| %>
        <tr>
          <td><%= dive.number %></td>
          <td><%= dive.height %>M</td>
          <td><%= dive.description %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>

