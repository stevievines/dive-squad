<%= form_for @current_coach do |f| %>
  <div class="form-group">
    <div class="field">
      <%= f.label :name %><br />
      <%= f.text_field :name, { autofocus: true, class: 'form-control' } %>
    </div>
  </div>

  <div class="form-group">
    <div class="field">
      <%= f.label :email %><br />
      <%= f.text_field :email, { class: 'form-control' } %>
    </div>
  </div>

  <div class="actions">
    <%= f.submit "Update", class: 'btn btn-primary' %>
  </div>
<% end %>
